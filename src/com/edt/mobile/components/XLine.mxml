<?xml version="1.0" encoding="utf-8"?>
<s:Line xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark">

	<s:stroke>
		<s:SolidColorStroke color="{color}"
							weight="1"/>
	</s:stroke>

	<fx:Script>
		<![CDATA[
			[Bindable]
			public var color:uint=0xAAAAAA;
		]]>
	</fx:Script>

</s:Line>
